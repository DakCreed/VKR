function reset_form(){
  var frm = document.forms["results"];
  deselect_options("rubric"); 
  deselect_options("title"); 
  deselect_options("org"); 
  deselect_options("author"); 
  deselect_options("year"); 
  deselect_options("type"); 
  deselect_options("role"); 
  deselect_options("keyword"); 
}
function pub_search(){
  var frm = document.forms["results"];
    frm.pagenum.value=1;
    frm.action="author_items.asp";
    frm.target="";
    frm.submit()
}


function itembox_add(){
  var frm = document.forms["results"];
  var issel=0;
  frm.items_all.value="";
  for (var i = 0; i < frm.elements.length; i++) {
    if (frm.elements[i].name.substring(0,3)=="tid"){
       if (frm.elements[i].checked) {
	     issel=issel+1;
	     frm.items_all.value = frm.items_all.value + frm.elements[i].name.substring(3, frm.elements[i].name.length) + "|";
	   }
    }
 }
 if (issel==0) {
    alerts ("Не выделено ни одной публикации")}
 else {
    $.fancybox({
      href : 'itembox_item_add2.asp?itemboxid='+frm.itemboxid.value+'&items_all='+frm.items_all.value ,
      title : '',
	  padding : 20,     
	  width : 480,
	  height : 150,
	  overlayOpacity : 0.3,	
	  overlayColor : '#000',
	  speedIn : 500,
	  hideOnOverlayClick : false,
	  enableEscapeButton : true,
	  type : 'iframe'
    });
  }
}

function items_rubric_add(){
  var frm = document.forms["results"];
  var issel=0;
  frm.items_all.value="";
  for (var i = 0; i < frm.elements.length; i++) {
    if (frm.elements[i].name.substring(0,3)=="tid"){
       if (frm.elements[i].checked) {
	     issel=issel+1;
	     frm.items_all.value = frm.items_all.value + frm.elements[i].name.substring(3, frm.elements[i].name.length) + "|";
	   }
    }
 }
 if (issel==0) {
    alerts ("Не выделено ни одной публикации")}
 else {
  $.fancybox({
    href : 'items_rubric_add.asp?items_all='+frm.items_all.value ,
    title : '',
	  padding : 20,     
	  width : 480,
	  height : 150,
	  overlayOpacity : 0.3,	
	  overlayColor : '#000',
	  speedIn : 500,
	  hideOnOverlayClick : false,
	  enableEscapeButton : true,
	  type : 'iframe'
  });
 }
}

function itembox_new(aid,anam){
  var frm = document.forms["results"];
  var el = document.createElement("OPTION");
  el.text=anam;
  el.value=aid;
  frm.itemboxid.options[frm.itemboxid.options.length]=el;
  frm.itemboxid[frm.itemboxid.length-1].selected=true;
}
function itembox_add_all(){
  var frm = document.forms["results"];
    $.fancybox({
      href : 'itembox_item_add2.asp?itemboxid='+frm.itemboxid.value+'&add_all=2' ,
      title : '',
	  padding : 20,     
	  width : 480,
	  height : 150,
	  overlayOpacity : 0.3,	
	  overlayColor : '#000',
	  speedIn : 500,
	  hideOnOverlayClick : false,
	  enableEscapeButton : true,
	  type : 'iframe'
    });
}
function newquery(){
  document.forms["results"].action="querybox.asp";
  document.forms["results"].target="";
  document.forms["results"].submit()
}
function show_help(topic){
  anewwin=window.open("", "addwin", "resizable=yes,scrollbars=yes,height=300,width=500,top=0,left=0");
  document.forms["results"].action=topic;
  document.forms["results"].target="addwin";
  document.forms["results"].submit()
}
function load_article(id){

	document.forms["results"].action="full_text.asp?id="+id;
	document.forms["results"].target="_blank";
	document.forms["results"].submit()

}
function url_article(idid, urlnum){

	document.forms["results"].action="url_article.asp";
	document.forms["results"].did.value=idid;
	document.forms["results"].urlnum.value=urlnum;
	document.forms["results"].target="_blank";
	document.forms["results"].submit()

}
function order_item(id, pr){

  var frm = document.forms["results"];
  if (0-pr>=0){
    ret = false;
    confirms("На Вашем счету сейчас 0 руб. \nПри открытии доступа к полному тексту данной публикации с Вашего счета будет списано " + eval(pr) + " руб.\nПодтверждаете приобретение публикации?", function(){
    if (ret) {
      anewwin=window.open("", "fulltext", "resizable=yes,scrollbars=yes,top=0,left=0");
      frm.action="buy_item.asp";
      frm.payitemid.value=id;
      frm.target="fulltext";
      frm.submit()
	  }
    })
  }
  else {
    $.get('basket_add.asp?id='+id+'&userid=5342270', function(data) {
    if (data!='') {
      var buy1txt='';
      var buy2txt='';
	  var n=data.indexOf('|');
      buy1txt=data.substring(0,n);
      buy2txt=data.substring(n+1);
      $('#pdf_'+id).html('<img src="/images/basket.png" width=16 height=16 vspace=3 border=0 title="Эта публикация уже в моей корзине ('+pr+' руб.)">');
      $('#basket_num').html(encodeURIComponent(buy1txt));
      $('#basket_sum').html(encodeURIComponent(buy2txt));
	  $('#win_basket').slideDown(500, function(){didScroll = true;});
	  alerts('Публикация добавлена в <a href="/basket.asp"><b>корзину</b></a>');
      }
    });
  }

}
function goto_page(pnum){
  var frm = document.forms["results"];
  frm.pagenum.value=pnum;
  frm.action="author_items.asp";
  frm.target="";
  frm.submit()
}

function author_items_print(){
  var frm = document.forms["results"];
  anewwin=window.open("", "author_items_print39190", "resizable=yes,scrollbars=yes,menubar=yes,height=600,width=700,top=50,left=50");
  frm.action="author_items_print.asp";
  frm.target="author_items_print39190";
  frm.submit()
}



function show_author_refs(iid){

  window.location.href="author_refs.asp?authorid=" + iid

}


function isVisible(tag) {
    var t = $(tag);
    var w = $(window);
    var wt = w.scrollTop();
    var tt = t.offset().top;
    var tb = tt + t.height();
    return ((tb <= wt + w.height()) && (tt >= wt));
}
function pdf_icon(b) {
  if (!b.prop("shown") && isVisible(b)) {
    b.prop("shown", true);
    b.load("ispdf.asp?itemid=" + b.attr('name').substring(4) + "&rand=" + Math.random());
  }
}
function get_pdfs(){
  $('div[id^="pdf_"]:not(prop("shown"))').each(function(){
    pdf_icon($(this))
  });
}
setInterval(function() {
  if ( didScroll2 ) {
	didScroll2=false;
	get_pdfs();
    }
 }, 200);

