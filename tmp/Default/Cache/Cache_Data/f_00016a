function load_article(iid){

	document.forms["results"].action="full_text.asp?id=" + iid;
	document.forms["results"].target="_blank";
	document.forms["results"].submit()

}
function show_contents(){

	document.forms["results"].action="show_contents.asp";
	document.forms["results"].target="_blank";
	document.forms["results"].submit()

}
function show_freepart(){

	document.forms["results"].action="show_freepart.asp";
	document.forms["results"].target="_blank";
	document.forms["results"].submit()

}
function file_article(fid, filenum){

	document.forms["results"].action="file_article.asp";
	document.forms["results"].filenum.value=filenum;
	document.forms["results"].fileid.value=fid;
	document.forms["results"].target="_blank";
	document.forms["results"].submit()

}
function url_article(idid, urlnum){

	document.forms["results"].action="url_article.asp";
	document.forms["results"].did.value=idid;
	document.forms["results"].urlnum.value=urlnum;
	document.forms["results"].target="_blank";
	document.forms["results"].submit()

}
function email_article(){

	if (document.forms["results"].email.value==''){
		alerts("Не указан адрес электронной почты!");}
	else{
		$.fancybox({
			href : '/email_article.asp?id=28808229&email='+document.forms["results"].email.value+'&rand=' + Math.random(),
			title : '',
			padding : 20,     
			width : 400,
			height : 150,
			overlayOpacity : 0.3,	
			overlayColor : '#000',
			speedIn : 500,
			hideOnOverlayClick : false,
			enableEscapeButton : true,
			type : 'iframe'
			});
		}

}
