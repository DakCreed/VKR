<script type="text/javascript">
function order_item(){

  $.get('basket_add.asp?id=28808229&userid=5342270', function(data) {
  if (data!='') {
    var buy1txt='';
    var buy2txt='';
	var n=data.indexOf('|');
    buy1txt=data.substring(0,n);
    buy2txt=data.substring(n+1);
    $('#buy1').html('<a href="basket.asp"><img src="images/basket.png" width=18 height=18 border=0></a>');
    $('#buy2').html('<a href="basket.asp">Эта публикация уже в моей корзине (750 руб.)</a>');
    $('#basket_num').html(encodeURIComponent(buy1txt));
    $('#basket_sum').html(encodeURIComponent(buy2txt));
	$('#win_basket').slideDown(500, function(){didScroll = true;});
	alerts('Публикация добавлена в <a href="/basket.asp"><b>корзину</b></a>');
  }
});

}
</script>
