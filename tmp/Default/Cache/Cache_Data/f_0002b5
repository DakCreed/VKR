$(document).ready(function() { 
	didScroll=true;



$('#rubric_order').change(function() { 
var selids="";
$('input:checkbox[id^="rubrics_"]:checked').each(function(){
  selids += $(this).attr('name').substring(8) + "|"});
$('#rubrics_options').html("<br>&nbsp;&nbsp;&nbsp;Загружается...");
$('#rubrics_options').load("author_items_rubrics.asp?id=1132834&order=" + $('#rubric_order').val() + "&selids=" + selids + "&show_hash=0&show_refs=1&show_option=1&hide_doubles=1&rand=" + Math.random());
rubricloaded=1;
})
$('*[id$="_rubrics"]').click(function() { 
hideShowSel1('rubrics');
if (rubricloaded==0){
$('#rubrics_options').html("<br>&nbsp;&nbsp;&nbsp;Загружается...");
$.get("author_items_rubrics.asp?id=1132834&order=" + $('#rubric_order').val() + "&selids=" + "&show_hash=0&show_refs=1&show_option=1&hide_doubles=1&rand=" + Math.random(), function(data) {
  $('#rubrics_options').html(data);
  if ($('#rubrics_table').height()<=200){$('#rubrics_options').height($('#rubrics_table').height()+10)}
  else {$('#rubrics_options').height(200)};
  rubricloaded=1;
  })
}
else {
  if ($('#rubrics_table').height()<=200){$('#rubrics_options').height($('#rubrics_table').height()+10)}
  else {$('#rubrics_options').height(200)};
}
})


