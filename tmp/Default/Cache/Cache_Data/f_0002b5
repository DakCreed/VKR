<SCRIPT LANGUAGE="JavaScript">
function end_session(){

  if (confirm("Сохранить Ваши данные на этом компьютере?")){
    document.forms["session"].saveuser.value="1";}
  else{
    document.forms["session"].saveuser.value="0";}

  document.forms["session"].action="/end_session.asp";
  document.forms["session"].target="";
  document.forms["session"].submit()
}
</SCRIPT>
<FORM action="/end_session.asp" method=POST name="session">
